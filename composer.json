{
    "name": "fast/framework",
    "description": "The fast Framework.",
    "keywords": ["framework", "fast"],
    "license": "MIT",
    "homepage": "https://github.com/flavorzyb/fast-framework",
    "support": {
        "issues": "https://github.com/flavorzyb/fast-framework/issues",
        "source": "https://github.com/flavorzyb/fast-framework"
    },
    "authors": [
        {
            "name": "Yanbin Zhu",
            "email": "haker-haker@163.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "ext-mbstring": "*",
        "ext-openssl": "*",
        "ext-fileinfo": "*",
        "ext-redis": "*",
        "nesbot/carbon": "~1.24.1",
        "ramsey/uuid": "^3.7",
        "moontoast/math": "^1.1",
        "ircmaxell/random-lib": "~1.2",
        "symfony/finder": "~4.0",
        "symfony/process": "~4.0",
        "symfony/routing": "~4.0",
        "psr/container": "~1.0",
        "doctrine/inflector": "~1.1",
        "psr/simple-cache": "^1.0",
        "smarty/smarty": "~3.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "php-coveralls/php-coveralls": "^2.0",
        "phpunit/phpcov": "^2.0",
        "mikey179/vfsStream": "~1.6",
        "mockery/mockery": "~1.0"
    },
    "autoload": {
        "files": [
            "src/Support/helpers.php"
        ],
        "psr-4": {
            "Fast\\": "src/"
        }
    },
    "autoload-dev": {
        "files": [
        ],
        "psr-4": {
            "Fast\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
